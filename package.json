{
  "name": "node-req",
  "version": "2.0.1",
  "description": "I/O parser for nodejs http request",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "./node_modules/.bin/nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "node-req",
    "req",
    "node-http-request",
    "http-request"
  ],
  "author": "amanvirk",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "nyc": "^11.4.1",
    "pem": "^1.12.3",
    "semver": "^5.5.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "include": [
      "index.js"
    ]
  },
  "dependencies": {
    "accepts": "^1.3.4",
    "fresh": "^0.5.2",
    "parseurl": "^1.3.2",
    "proxy-addr": "^2.0.2",
    "qs": "^6.5.1",
    "type-is": "^1.6.15"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/node-req.git"
  },
  "bugs": {
    "url": "https://github.com/poppinss/node-req/issues"
  },
  "homepage": "https://github.com/poppinss/node-req#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
